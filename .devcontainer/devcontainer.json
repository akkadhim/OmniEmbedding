{
    "name": "omni_embedding",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/workspace",
    "mounts": [
      "source=/raid/home/${localEnv:USER}/embedding_files,target=/workspace/mnt/data,type=bind"
    ],
    "forwardPorts": [],
    "postCreateCommand": "echo 'Devcontainer is ready'",
    "remoteUser": "root",
    "customizations": {
      "jetbrains": {
        "plugins" : [
          "com.github.copilot",
          "Pythonid"
        ],
  
      },
      "vscode": {
        "extensions": [
          "ms-python.python",
          "ms-python.debugpy",
          "donjayamanne.python-extension-pack",
          "donjayamanne.python-environment-manager"
        ]
      }
    }
  }